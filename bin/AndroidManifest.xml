<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pumpkinonlinedisk"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="21" />
    
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    

    <application
        android:allowBackup="true"
        android:icon="@drawable/tubiao"
        android:label="@string/app_name"
        android:theme="@android:style/Theme.Holo.Light.NoActionBar" android:allowTaskReparenting="false">
        <activity
            android:name="com.example.pumpkinonlinedisk.aty.Initial_Activity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".aty.Land"></activity>
        <activity android:name=".aty.Register"></activity>
        <activity android:name=".aty.MainActivity"></activity>
        <service android:name="com.example.pumpkinonlinedisk.service.DownAndUploadService"></service>
        <activity android:name=".aty.AtyPhotoViewer"></activity>
    </application>
    

</manifest>
